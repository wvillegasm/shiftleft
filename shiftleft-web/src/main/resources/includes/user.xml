<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- - This file is included by the struts.xml file as an example - of how 
	to break up the configuration file into multiple files. -->
<struts>
	<package name="users" namespace="/user" extends="struts-default">
		<action name="*" class="org.shiftleft.web.action.UserAction" method="{1}">
			<result name="input">/WEB-INF/user/input.jsp</result>
			<result>/WEB-ING/user/show.jsp</result>
		</action>
	</package>
</struts>